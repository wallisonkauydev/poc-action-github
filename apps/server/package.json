{
  "name": "server",
  "version": "1.0.0",
  "description": "backend",
  "main": "server.js",
  "scripts": {
    "build": "tsc --build",
    "watch": "tsc -w",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:migrate:undo:all": "sequelize db:migrate:undo:all",
    "db:seed": "sequelize db:seed:all",
    "db:redo": "yarn build && yarn db:migrate && yarn db:seed",
    "startWithNewRelic": "NEW_RELIC_APP_NAME=sacflow-server NEW_RELIC_LICENSE_KEY=85552c1ab4fb92c55b456f3315adcee9FFFFNRAL node -r newrelic dist/server.js",
    "start": "node dist/server.js",
    "start:filas": "nodemon dist/queues.js",
    "dev": "ts-node-dev  --respawn --transpile-only --ignore node_modules src/server.ts",
    "dev:sync-messages": "ts-node-dev  --transpile-only --ignore node_modules src/libs/typesense/sync-local-messages.ts",
    "dev:debug": "ts-node-dev --transpile-only --respawn --inspect=4321 --project tsconfig.json src/server.ts",
    "worker": "ts-node-dev  --respawn --transpile-only --ignore node_modules src/worker.ts",
    "worker:debug": "ts-node-dev --transpile-only --respawn --inspect=4321 --project tsconfig.json src/worker.ts",
    "filas": "ts-node-dev --respawn --transpile-only --ignore node_modules src/queues.ts",
    "autocannon": "node src/autocannon/envia-mensagens.js",
    "test:e2e": "jest -i --testPathPattern=src/__tests__/e2e --silent --forceExit",
    "test:e2e:cov": "jest -i --testPathPattern=src/__tests__/e2e --silent --forceExit --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src --fix --quiet",
    "test": "jest --forceExit --silent"
  },
  "author": "Hyerdev",
  "license": "MIT",
